{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-redis": "*",
        "doctrine/cache": "^1.11",
        "doctrine/annotations": "^1.12",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.8",
        "friendsofsymfony/elastica-bundle": "6.0.x-dev",
        "nyholm/psr7": "^1.4",
        "php-amqplib/rabbitmq-bundle": "^2.7",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^6.1",
        "sentry/sentry-symfony": "^4.1",
        "symfony/asset": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/expression-language": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/http-client": "5.2.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/property-access": "5.2.*",
        "symfony/property-info": "5.2.*",
        "symfony/psr-http-message-bridge": "^2.1",
        "symfony/security-bundle": "5.2.*",
        "symfony/serializer": "5.2.*",
        "symfony/validator": "5.2.*",
        "symfony/yaml": "5.2.*"
    },
    "require-dev": {
        "lctrs/psalm-psr-container-plugin": "^1.1",
        "overtrue/phplint": "^2.3",
        "psalm/plugin-symfony": "^2.2",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/debug-bundle": "5.2.*",
        "symfony/maker-bundle": "^1.29",
        "symfony/stopwatch": "^5.2",
        "symfony/twig-bundle": "^5.2",
        "symfony/web-profiler-bundle": "^5.2",
        "vimeo/psalm": "^4.6",
        "weirdan/doctrine-psalm-plugin": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "lint": "XDEBUG_MODE=off phplint",
        "phpcs": "XDEBUG_MODE=off phpcs --standard=phpcs.xml",
        "phpcbf": "XDEBUG_MODE=off phpcbf --standard=phpcs.xml",
        "psalm": "XDEBUG_MODE=off psalm --config=psalm.xml",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
